{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      },
      "channel": "development"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "channel": "ios-simulator"
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "autoIncrement": true,
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1"
        },
        "simulator": false
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}